{
  "compilerOptions": {
    "module": "ES2020",
    "target": "ES2020",
    "outDir": "./dist",
    "lib": ["es5","es6","dom"],
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": ["src"],
  "exclude": ["node_modules","dist"]
}
  